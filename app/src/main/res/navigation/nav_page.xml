<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_page"
    app:startDestination="@id/main_home">

    <fragment
        android:id="@+id/main_home"
        android:name="com.example.companymanagement.view.fragment.mainhome.MainHome"
        android:label="fragment_home"
        tools:layout="@layout/fragment_main_home" >
        <action
            android:id="@+id/action_main_home_to_main_project"
            app:destination="@id/main_project" />
    </fragment>
    <fragment
        android:id="@+id/main_project"
        android:name="com.example.companymanagement.view.fragment.mainworkspace.MainWorkspace"
        android:label="fragment_main_workspace"
        tools:layout="@layout/fragment_main_project" >
        <action
            android:id="@+id/action_main_project_to_main_workspace"
            app:destination="@id/main_workspace" />
    </fragment>
    <fragment
        android:id="@+id/main_workspace"
        android:name="com.example.companymanagement.view.fragment.mainworkspace.MainWorkspace"
        android:label="fragment_main_workspace"
        tools:layout="@layout/fragment_main_workspace" >
        <action
            android:id="@+id/action_main_workspace_to_main_more"
            app:destination="@id/main_more" />
    </fragment>
    <fragment
        android:id="@+id/main_more"
        android:name="com.example.companymanagement.view.fragment.mainworkspace.MainWorkspace"
        android:label="fragment_main_workspace"
        tools:layout="@layout/fragment_main_more" />

</navigation>